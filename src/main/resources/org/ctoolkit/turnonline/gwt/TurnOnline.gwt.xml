<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8//EN"
        "https://github.com/gwtproject/gwt/raw/2.8.0/distro-source/core/src/gwt-module.dtd">

<module rename-to="turnonline.gwt">
    <inherits name="com.google.gwt.user.User"/>
    <inherits name="com.google.gwt.activity.Activity"/>
    <inherits name="com.google.web.bindery.autobean.AutoBean"/>
    <inherits name="com.google.web.bindery.requestfactory.RequestFactory"/>

    <extend-property name="locale" values="sk"/>

    <source path="client"/>

</module>
